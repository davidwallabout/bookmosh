{
  "expo": {
    "name": "BookMosh",
    "slug": "bookmosh-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookmosh.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "BookMosh needs camera access to let you take photos for your profile.",
        "NSPhotoLibraryUsageDescription": "BookMosh needs photo library access to let you choose a profile picture."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bookmosh.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "owner": "bookmosh"
  }
}
